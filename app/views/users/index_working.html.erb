<% provide(:title, "出勤社員一覧") %>

<h1>出勤社員一覧</h1>

<table class="table table-bordered table-striped">
  <thead>
    <tr>
      <th class="center">社員番号</th>
      <th class="center">名前</th>
    </tr>
  </thead>
  <tbody>
    <% @users.each do |user| %>
      <tr>
        <td><%= user.employee_number %></td>
        <td><%= user.name %></td>
      </tr>
    <% end %>
  </tbody>
  
</table>